{
  "name": "dtomaker",
  "displayName": "dtomaker",
  "description": "",
  "version": "0.1.2",
  "publisher": "julie",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:dtomaker.all",
    "onCommand:dtomaker.alldto",
    "onCommand:dtomaker.onedto",
    "onCommand:dtomaker.allcreatesql",
    "onCommand:dtomaker.allsqlite",
    "onCommand:dtomaker.onesqlite"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Julie-ver5/dtomaker.git"
  },
  "main": "./out/extension.js",
  "icon": "images/icon.png",
  "contributes": {
    "commands": [
      {
        "command": "dtomaker.all",
        "title": "%dtomaker.all.title%",
        "category": "DTO Maker"
      },
      {
        "command": "dtomaker.alldto",
        "title": "%dtomaker.alldto.title%",
        "category": "DTO Maker"
      },
      {
        "command": "dtomaker.onedto",
        "title": "%dtomaker.onedto.title%",
        "category": "DTO Maker"
      },
      {
        "command": "dtomaker.allcreatesql",
        "title": "%dtomaker.allcreatesql.title%",
        "category": "DTO Maker"
      },
      {
        "command": "dtomaker.allsqlite",
        "title": "%dtomaker.allsqlite.title%",
        "category": "DTO Maker"
      },
      {
        "command": "dtomaker.onesqlite",
        "title": "%dtomaker.onesqlite.title%",
        "category": "DTO Maker"
      }
    ],
    "configuration": [
      {
        "title": "DTOMaker configuration"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.59.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "mysql2": "^2.3.0"
  }
}
